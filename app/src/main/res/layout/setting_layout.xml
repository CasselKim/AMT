<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ebf2f5"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/layout_bottom"
        android:layout_width="393dp"
        android:layout_height="66dp"
        android:layout_marginTop="580dp"
        android:background="#8EC0E4"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/list_Button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="580dp"
        android:background="#00ffffff"
        android:onClick="onClick01"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_list_black_36dp" />

    <ImageButton
        android:id="@+id/map_Button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="580dp"
        android:background="#00ffffff"
        android:onClick="onClick02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_map_black_36dp" />

    <ImageButton
        android:id="@+id/weather_Button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="582dp"
        android:background="#00ffffff"
        android:onClick="onClick03"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_wb_sunny_black_36dp" />

    <ImageButton
        android:id="@+id/setting_Button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="320dp"
        android:layout_marginTop="580dp"
        android:background="#00ffffff"
        android:onClick="onClick04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_settings_applications_black_36dp" />

    <TextView
        android:id="@+id/status1"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="14dp"
        android:text="Setting"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_marginTop="70dp"
        android:fontFamily=""
        android:textSize = "17sp"
        android:text="알림 유형(시간/위치)                                              "
        android:textColor="#000000"
        android:background="#ebf2f5"
        android:drawableLeft="@drawable/baseline_alarm_black_18dp"
        android:paddingLeft="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_marginTop="120dp"
        android:background="#ebf2f5"
        android:drawableLeft="@drawable/baseline_mail_outline_black_18dp"
        android:fontFamily=""
        android:paddingLeft="15dp"
        android:text="메시지 알림                                                               "
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_marginTop="170dp"
        android:fontFamily=""
        android:textSize = "17sp"
        android:text="소리                                                                             "
        android:textColor="#000000"
        android:background="#ebf2f5"
        android:drawableLeft="@drawable/baseline_notifications_black_18dp"
        android:paddingLeft="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/button5"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_marginTop="220dp"
        android:fontFamily=""
        android:textSize = "17sp"
        android:text="진동                                                                             "
        android:textColor="#000000"
        android:background="#ebf2f5"
        android:drawableLeft="@drawable/baseline_notifications_active_black_18dp"
        android:paddingLeft="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>